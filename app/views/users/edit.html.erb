<div>
  <%= simple_form_for [@user] do |f| %>
      <%= f.input :user_name %>
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :email %>
      <%= f.input :pronoun %>
      <%= f.input :image %>
      <%= f.input :description %>
      <%= f.button :submit, "Update", class: "btn-submit"%>
    <% end %>
  <li><%= link_to 'Edit', edit_user_path(@user) %></li>

  </div>
</div>
