<div class="card-container" >
  <div class="container">
    <div class="map-card bg-dark border border-white">
      <img src="<%= user.booker.image %>" alt="" style="width: 150px; height: 150px; object-fit: cover; border-radius: 50%;"> <br>
      <div class="map-card-text">
        <h5><%= user.date.to_formatted_s(:rfc822)[0..-10] %></h5><br>
        <h6><%= user.venue %></h6>
    </div>
  </div>
</div>
