<%# <br><br><br><br><br> %>
<div class="main_log" style="background-color: black;">
<br><br><br><br>
  <div class="title">Log-In</div>
  <div class="log_in">
    <div class="logo">
      <img src="https://i.imgur.com/arALlu1.png" alt="" width="100">
    </div>
    <div class="title">Bite Jockey</div>
    <div class="subtitle">Less Admin, More Mixin</div>
    <div class="form">
      <div class="email">
      <input type="text" placeholder="Email">
      </div>
      <div class="password">
      <input type="password" placeholder="Password">
      </div>
      <div class="first_name">
      <input type="text" placeholder="First Name">
      </div>
      <div class="last_name">
      <input type="text" placeholder="Last Name">
      </div>
      <div class="username">
      <input type="text" placeholder="Username">
      </div>
      <div class="operator_type">
      <input type="text" placeholder="Operator Type">
      </div>
      <div class="location">
      <input type="text" placeholder="Location">
      </div>
      <div class="image">
      <input type="text" placeholder="Image">
      </div>
      <div class="social_link">
      <input type="text" placeholder="Social Link">
      </div>
      <div class="options">
        <div class="forgot-password">
          <a href="#">Forgot Password?</a>
        </div>
      </div>
      <div class="sign-up">
        <button class="glow-on-hover">Log In</button>
      </div>
      <div class="sign-up">
        <button class="glow-on-hover">Sign Up</button>
      </div>
    </div>
  </div>
  <br><br>
</div>
